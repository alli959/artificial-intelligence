   "source": [
    "import numpy as np\n",
    "\n",
    "def rowsum(A):\n",
    "    rs = []\n",
    "    for i in A:\n",
    "        s = sum(i)\n",
    "        rs.append(s)\n",
    "    \n",
    "    return rs\n",
    "\n",
    "# Test\n",
    "A=np.array([[1, 2, 3], [3, 4, -5]])\n",
    "print(\"rowsum(A) :\", rowsum(A)) # Outputs [6, 2]"
